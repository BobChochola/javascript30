var categoryId;
var deliveryIds;
var productIds;
var paymentIds;
var giftIds;
var promotionIds;
var freeshippingIds;
var merchantId = window.__PRELOADED_STATE__.merchantData._id
var mediaId = window.__PRELOADED_STATE__.merchantData.favicon_media._id
var env = window.origin
// var env = "https://admin.shoplineapp.com"
var token = $('[name="csrf-token"]').attr("content")
//stg superAdmin
var userId = window.__PRELOADED_STATE__.currentUser._id

$.ajax({
    type: 'GET',
    url: `${env}/api/admin/v1/${merchantId}/categories?`,
    dataType: 'json',
    success: function(data) {  categoryId = `"${data.data.items[0]._id}"` },
    async: false
});
// console.log(categoryId)

function filterPayment(payments){
    var array = []
      payments.forEach((payment)=>{
        paymentIds.forEach((paymentId)=>{
          if(paymentId.name_translations.en == payment){
            return array.push(`"${paymentId._id}"`)
          }
        })
      })      
    return array
}
// console.log(filterPayment(['Taishin', 'Pay on Family Mart Store Pickup (B2C)']))

function filterDelivery(deliverys){
    var array = []
      deliverys.forEach((delivery)=>{
        deliveryIds.forEach((deliveryId)=>{
          if(deliveryId.name_translations.en == delivery){
            return array.push(`"${deliveryId._id}"`)
          }
        })
      })
    return array
}
// console.log(filterDelivery(['Local Delivery', 'Family Mart Pickup only (C2C)']))

function filterProduct(products){
    var array = []
      products.forEach((product)=>{
        productIds.forEach((productId)=>{
          if(productId.title_translations.en == product){
            return array.push(`"${productId._id}"`)
          }
        })
      })
    return array
}
// console.log(filterProduct(['Out Of Stock', '20th Product for Checkout']))
function filterGift(gifts){
    var array = []
      gifts.forEach((gift)=>{
        giftIds.forEach((giftId)=>{
          if(giftId.title_translations.en == gift){
            console.log(giftId)
            return array.push(`"${giftId._id}"`)
          }
        })
      })
    return array
}
// console.log(filterGift(['1st Created Free Gift']))

