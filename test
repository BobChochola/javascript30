createDelivery()
createPayment()
}

function create2(){
$.ajax({
    type: 'GET',
    url: `${env}/api/admin/v1/${merchantId}/delivery_options?`,
    dataType: 'json',
    success: function(data) {  deliveryIds = data.items },
    async: false
});
$.ajax({
    type: 'GET',
    url: `${env}/api/admin/v1/${merchantId}/payments?`,
    dataType: 'json',
    success: function(data) {  paymentIds = data.data.items },
    async: false
});
createProduct()
createFreegift()
}

