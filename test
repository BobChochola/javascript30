createAddon()
}

function unpublishPromotions(){


$.ajax({
    type: 'GET',
    url: `${env}/api/admin/v1/${merchantId}/promotions?&types[]=percentage&types[]=amount&types[]=gift&limit=100&scope=all`,
    dataType: 'json',
    success: function(data) {  promotionIds = data.data.items },
    async: false
});
$.ajax({
    type: 'GET',
    url: `${env}/api/admin/v1/${merchantId}/promotions?&types[]=free_shipping&limit=10&page=1&scope=all`,
    dataType: 'json',
    success: function(data) {  freeshippingIds = data.data.items },
    async: false
});
promotionIds.forEach((promotionId)=>{

fetch(`${env}/api/admin/v1/${merchantId}/promotions/${promotionId._id}/unpublish`, {
  "headers": {
    "accept": "application/json, text/plain, */*",
    "accept-language": "en-US,en;q=0.9,zh-TW;q=0.8,zh;q=0.7",
    "sec-fetch-dest": "empty",
    "sec-fetch-mode": "cors",
    "sec-fetch-site": "same-origin",
    "x-csrf-token": token,
    "x-requested-with": "XMLHttpRequest"
  },
  "referrer": `${env}/admin/${merchantId}/promotions/discount/`,
  "referrerPolicy": "strict-origin-when-cross-origin",
  "body": null,
  "method": "PUT",
  "mode": "cors",
  "credentials": "include"
});
})

freeshippingIds.forEach((freeshippingId)=>{

fetch(`${env}/api/admin/v1/${merchantId}/promotions/${freeshippingId._id}/unpublish`, {
  "headers": {
    "accept": "application/json, text/plain, */*",
    "accept-language": "en-US,en;q=0.9,zh-TW;q=0.8,zh;q=0.7",
    "sec-fetch-dest": "empty",
    "sec-fetch-mode": "cors",
    "sec-fetch-site": "same-origin",
    "x-csrf-token": token,
    "x-requested-with": "XMLHttpRequest"
  },
  "referrer": `${env}/admin/${merchantId}/promotions/free_shipping/`,
  "referrerPolicy": "strict-origin-when-cross-origin",
  "body": null,
  "method": "PUT",
  "mode": "cors",
  "credentials": "include"
});
})

}


fetch(`${env}/api/admin/v1/${merchantId}`, {
  "headers": {
    "accept": "application/json, text/plain, */*",
    "accept-language": "en-US,en;q=0.9,zh-TW;q=0.8,zh;q=0.7",
    "content-type": "application/json;charset=UTF-8",
    "x-csrf-token": token,
    "x-requested-with": "XMLHttpRequest"
  },
  "referrer": `${env}/admin/${merchantId}/edit`,
  "referrerPolicy": "strict-origin-when-cross-origin",
  "body": "{\"merchant\":{\"name\":\"test shop\",\"email\":\"testuser+2018082101550@shoplineapp.com\",\"emails\":{\"order\":\"\",\"message\":\"\",\"comment\":\"\",\"low_inventory\":\"\",\"billing\":[]},\"base_currency_code\":\"MYR\",\"base_country_code\":\"MY\",\"force_update_country_code\":false,\"default_language_code\":\"en\",\"ignore_browser_lang\":true,\"shop_status\":\"open\",\"phone_number\":\"0912345678\",\"tags\":[],\"supported_languages\":[\"en\",\"zh-hant\"]}}",
  "method": "PUT",
  "mode": "cors",
  "credentials": "include"
});
create1()
setTimeout(function(){
  create2()
  setTimeout(function(){
    create3()
