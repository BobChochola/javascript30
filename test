/*-----------------------------------------------------------------addon-----------------------------------------------------------------*/
function createAddon(){
  //1st Addon for Checkout
  fetch(`${env}/api/admin/v1/${merchantId}/addon_products`, {
    "headers": {
      "accept": "application/json, text/plain, */*",
      "accept-language": "en-US,en;q=0.9,zh-TW;q=0.8,zh;q=0.7",
      "content-type": "application/json;charset=UTF-8",
      "sec-fetch-dest": "empty",
      "sec-fetch-mode": "cors",
      "sec-fetch-site": "same-origin",
      "x-csrf-token": token,
      "x-requested-with": "XMLHttpRequest"
    },
    "referrer": `${env}/admin/${merchantId}/addon_items/new`,
    "referrerPolicy": "strict-origin-when-cross-origin",
    "body": `{\"addon_product\":{\"title_translations\":{\"en\":\"1st Addon for Checkout\"},\"description_translations\":{},\"unlimited_quantity\":false,\"main_products\":[{\"addon_price\":{\"dollars\":10},\"_id\":${filterProduct(['1st Product for Checkout'])}}],\"quantity\":2000000,\"media_ids\":[\"${mediaId}\"],\"is_same_price\":false,\"addon_price\":null,\"start_at\":null,\"end_at\":null},\"performer_id\":\"${userId}\"}`,
    "method": "POST",
    "mode": "cors",
    "credentials": "include"
  });
  //2nd Addon for Checkout
  fetch(`${env}/api/admin/v1/${merchantId}/addon_products`, {
    "headers": {
      "accept": "application/json, text/plain, */*",
      "accept-language": "en-US,en;q=0.9,zh-TW;q=0.8,zh;q=0.7",
      "content-type": "application/json;charset=UTF-8",
      "sec-fetch-dest": "empty",
      "sec-fetch-mode": "cors",
      "sec-fetch-site": "same-origin",
      "x-csrf-token": token,
      "x-requested-with": "XMLHttpRequest"
    },
    "referrer": `${env}/admin/${merchantId}/addon_items/new`,
    "referrerPolicy": "strict-origin-when-cross-origin",
    "body": `{\"addon_product\":{\"title_translations\":{\"en\":\"2nd Addon for Checkout\"},\"description_translations\":{},\"unlimited_quantity\":true,\"main_products\":[{\"addon_price\":{\"dollars\":10},\"_id\":${filterProduct(['4th Product for Checkout'])}},{\"addon_price\":{\"dollars\":10},\"_id\":${filterProduct(['8th Product for Checkout'])}}],\"quantity\":null,\"media_ids\":[\"${mediaId}\"],\"is_same_price\":false,\"addon_price\":null,\"start_at\":null,\"end_at\":null},\"performer_id\":\"${userId}\"}`,
    "method": "POST",
    "mode": "cors",
    "credentials": "include"
  });
  //3rd Addon for Checkout
  fetch(`${env}/api/admin/v1/${merchantId}/addon_products`, {
    "headers": {
      "accept": "application/json, text/plain, */*",
      "accept-language": "en-US,en;q=0.9,zh-TW;q=0.8,zh;q=0.7",
      "content-type": "application/json;charset=UTF-8",
      "sec-fetch-dest": "empty",
      "sec-fetch-mode": "cors",
      "sec-fetch-site": "same-origin",
      "x-csrf-token": token,
      "x-requested-with": "XMLHttpRequest"
    },
    "referrer": `${env}/admin/${merchantId}/addon_items/new`,
    "referrerPolicy": "strict-origin-when-cross-origin",
    "body": `{\"addon_product\":{\"title_translations\":{\"en\":\"3rd Addon for Checkout\"},\"description_translations\":{},\"unlimited_quantity\":false,\"main_products\":[{\"addon_price\":{\"dollars\":10},\"_id\":${filterProduct(['13th Product for Checkout'])}},{\"addon_price\":{\"dollars\":10},\"_id\":${filterProduct(['17th Product for Checkout'])}}],\"quantity\":2000000,\"sku\":\"000\",\"weight\":\"5.5\",\"media_ids\":[\"${mediaId}\"],\"is_same_price\":false,\"addon_price\":null,\"start_at\":null,\"end_at\":null},\"performer_id\":\"${userId}\"}`,
    "method": "POST",
    "mode": "cors",
    "credentials": "include"
  });
}

