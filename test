/*---------------------------------------------------------------freegift---------------------------------------------------------------*/

function createFreegift(){
  //gift
  fetch(`${env}/api/admin/v1/${merchantId}/gifts`, {
    "headers": {
      "accept": "application/json, text/plain, */*",
      "accept-language": "en-US,en;q=0.9,zh-TW;q=0.8,zh;q=0.7",
      "content-type": "application/json;charset=UTF-8",
      "sec-fetch-dest": "empty",
      "sec-fetch-mode": "cors",
      "sec-fetch-site": "same-origin",
      "x-csrf-token": token,
      "x-requested-with": "XMLHttpRequest"
    },
    "referrer": `${env}/admin/${merchantId}/free_gifts/new`,
    "referrerPolicy": "strict-origin-when-cross-origin",
    "body": `{\"gift\":{\"title_translations\":{\"en\":\"1st Created Free Gift\"},\"unlimited_quantity\":false,\"quantity\":9999999,\"weight\":\"5\",\"media_ids\":[\"${mediaId}\"]},\"performer_id\":\"${userId}\"}`,
    "method": "POST",
    "mode": "cors",
    "credentials": "include"
  });

  fetch(`${env}/api/admin/v1/${merchantId}/gifts`, {
    "headers": {
      "accept": "application/json, text/plain, */*",
      "accept-language": "en-US,en;q=0.9,zh-TW;q=0.8,zh;q=0.7",
      "content-type": "application/json;charset=UTF-8",
      "sec-fetch-dest": "empty",
      "sec-fetch-mode": "cors",
      "sec-fetch-site": "same-origin",
      "x-csrf-token": token,
      "x-requested-with": "XMLHttpRequest"
    },
    "referrer": `${env}/admin/${merchantId}/free_gifts/new`,
    "referrerPolicy": "strict-origin-when-cross-origin",
    "body": `{\"gift\":{\"title_translations\":{\"en\":\"2nd Created Free Gift\"},\"unlimited_quantity\":true,\"quantity\":null,\"media_ids\":[\"${mediaId}\"]},\"performer_id\":\"${userId}\"}`,
    "method": "POST",
    "mode": "cors",
    "credentials": "include"
  });

}

